.slider swiper{
  width: 100%;
  height: 374rpx;
  overflow: hidden;
}
.slider image{
  width: 100%;
  height: 374rpx;
}
.nav-item image{
  width: 100rpx;
  height: 100rpx;
}
.index-nav .nav-item{
  width: 33.3333%;
  height: 200rpx;
  border-bottom: 1rpx solid #ccc;
  border-right: 1rpx solid #ccc;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  font-size: 14px;
}
.index-nav .nav-item:nth-child(3n){
  border-right: 0 none;
}
.index-nav{
  display: flex;
  flex-wrap: wrap;
}
.index-enter image{
  width: 320rpx;
  height: 178rpx;
}
.index-enter{
  display: flex;
  padding: 20rpx 30rpx;
}
.index-enter navigator{
  width: 50%;
  display: flex;
  justify-content: center;
}
